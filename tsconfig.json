{
  "compilerOptions": {
    "allowJs": false,
    "target": "es5",
    "lib": ["es2015", "esnext.asynciterable"],
    "jsx": "react-native",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "types": [
      "jest"
    ],
    "baseUrl": "src",
    "paths": {
      "components/*": ["./components/*"],
      "styles/*": ["./styles/*"],
      "screens/*": ["./screens/*"]
    }
  }
}
